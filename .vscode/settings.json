{
    "python.testing.pytestArgs": [
        "apps"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "flake8.args": [
        "--max-line-length=90"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "icon": "bug"
        },
        "HACK": {
            "icon": "tools"
        },
        "FIXME": {
            "icon": "flame",
            "foreground": "#ffffff",
            "background": "#ff0000"
        },
        "XXX": {
            "icon": "x"
        },
        "[ ]": {
            "icon": "issue-draft"
        },
        "[x]": {
            "icon": "issue-closed"
        }
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/.venv/*/**",
        "**/README.md",
    ],
    "cSpell.words": [
        "ACKS",
        "activatable",
        "appname",
        "asgi",
        "autoretry",
        "Browsable",
        "celerybeat",
        "corsheaders",
        "createsuperuser",
        "devcontainers",
        "djangorestframework",
        "DJOSER",
        "dotenv",
        "escrowtimeout",
        "filterset",
        "Flutterwave",
        "Gruntfuggly",
        "gunicorn",
        "healthcheck",
        "ipykernel",
        "Jajjage",
        "keyspace",
        "levelname",
        "logfile",
        "loglevel",
        "logoutall",
        "Mailgun",
        "makemigrations",
        "Mustapha",
        "nostatic",
        "pidfile",
        "productdetail",
        "psycopg",
        "pycache",
        "pyproject",
        "pytest",
        "PYTHONDONTWRITEBYTECODE",
        "PYTHONUNBUFFERED",
        "querysets",
        "recalc",
        "repr",
        "runserver",
        "sadd",
        "safetrade",
        "simplejwt",
        "smembers",
        "sorl",
        "srem",
        "viewset",
        "viewsets",
        "VIRTUALENVS",
        "vscodeintellicode",
        "vtype",
        "Watchlist",
        "watchlists",
        "whitenoise",
        "wlgjuo",
        "wrongpassword",
        "wsgi"
    ],
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "driver": "PostgreSQL",
            "name": "safe trade",
            "database": "postgres",
            "username": "postgres"
        }
    ]
}